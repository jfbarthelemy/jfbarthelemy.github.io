@inproceedings{ibouricheneRING2020,
  title = {Investigating the Impact of Fracture Orientation Deviation on Elastic Effective Properties Using the Non-Periodic Homogenization},
  booktitle = {{{RING Meeting}}, {{ASGA}}},
